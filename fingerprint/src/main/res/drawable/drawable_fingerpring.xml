<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
            <stroke
                android:width="1dp"
                android:color="@android:color/darker_gray" />

        </shape>
    </item>
    <item>
        <bitmap android:src="@drawable/ic_fingerprint_black_24dp" />
    </item>

</layer-list>